<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:h="urn:import:com.google.gwt.user.client.ui">
    <ui:style>
        .footer {
            height: 40px;
            text-align: right;
            vertical-align: middle;
            padding-right: 10px;
        }
    </ui:style>

    <g:HTMLPanel>
        <b:PanelFooter addStyleNames="{style.footer} text-muted">
            <h:HTMLPanel>
                <small>Русские шашки</small>
                <br/>
                <small>Сайт разработан lynx-r</small>
            </h:HTMLPanel>
        </b:PanelFooter>
    </g:HTMLPanel>
</ui:UiBinder>