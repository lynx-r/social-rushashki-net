<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'>

    <ui:with field='playCellProxy' type="net.rushashki.shashki64.client.cell.proxy.PlayCellProxy"/>

    <div ui:field="playCell">
        <div ui:field="firstName">
            First name:
            <ui:text from="{playCellProxy.getTitle}"/>
        </div>
        Start date:
        <ui:text from="{playCellProxy.getWhitePlayer}"/>
        <ui:safehtml from="{playCellProxy.getDeskHtml}"/>
    </div>
</ui:UiBinder>